{
  "name": "cm.holidays",
  "version": "1.0.0",
  "description": "Allow person in the company to ask for a holiday that the company allow. Also to the responsible to validate this or refuse.",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --exec ts-node ./src/index.ts",
    "migration:generate": "typeorm-ts-node-commonjs -d ./src/data-source.ts migration:generate ./src/migrations/default",
    "migration:run": "typeorm-ts-node-commonjs -d ./src/data-source.ts migration:run",
    "openapi-generate": "openapi --input ./specs/holiday.yaml --output ./src/entities/services --client axios"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com-IT-WIBRC:IT-WIBRC/cm.holidays.git"
  },
  "keywords": [
    "Holidays",
    "RH"
  ],
  "author": "@IT-WIBRC",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.5.9",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "nodemon": "^3.0.1",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^10.9.1",
    "typeorm": "^0.3.17",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "openapi-typescript-codegen": "^0.25.0"
  }
}
